<!DOCTYPE html>
<html>
    <head>
        <title>Test Store</title>
        <base href="/" target="_top"/>
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
        <link href="<%= baseUrl %>/checkout.css" rel="stylesheet" type="text/css">
        <link href="https://fonts.googleapis.com/css?family=Karla:400|Montserrat:400,500,700&display=swap" rel="stylesheet" />
        <script type="text/javascript">
            window.language = {locale: "en", locales: {}, translations: {}}
        </script>
    </head>
    <body>
    <header class="checkoutHeader optimizedCheckout-header">
        <div class="checkoutHeader-content">
            <h1 class="is-srOnly">Checkout</h1>
            <h2 class="checkoutHeader-heading">
                <a class="checkoutHeader-link" href="<%= baseUrl %>/">
                    <span class="header-logo-text">Test Store</span>
                </a>
            </h2>
        </div>
    </header>
    <div ng-jq class="ng-checkout-container" id="micro-app-ng-checkout">
        <div class="ng-checkout-view" data-ui-view></div>
    </div>
    <script src="<%= baseUrl %>/runtime.js" type="text/javascript"></script>
    <script src="<%= baseUrl %>/polyfill~checkout.js" type="text/javascript"></script>
    <script src="<%= baseUrl %>/transients~checkout.js" type="text/javascript"></script>
    <script src="<%= baseUrl %>/sentry~checkout.js" type="text/javascript"></script>
    <script src="<%= baseUrl %>/vendors~checkout.js" type="text/javascript"></script>
    <script src="<%= baseUrl %>/checkout.js" type="text/javascript"></script>
    <script type="text/javascript">
        window.checkoutVariantIdentificationToken = '*';
        (function () {
            window.checkout.renderCheckout({
                checkoutId: '<%= checkoutId %>',
                containerId: 'micro-app-ng-checkout',
                publicPath: '<%= baseUrl %>/',
                sentryConfig: null,
            })
        })();
    </script>
    </body>
</html>
